import java.io.IOException;

public class Calculadora {

    public static void main(String[] args) {
        try {
            // Crear el proceso que abre la calculadora en Windows
            ProcessBuilder processBuilder = new ProcessBuilder("calc.exe");
            
            // Iniciar el proceso
            processBuilder.start();
            
            System.out.println("Calculadora abierta correctamente.");
            
        } catch (IOException e) {
            System.out.println("Error al intentar abrir la calculadora.");
            e.printStackTrace();
        }
    }
}
